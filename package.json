{
  "name": "white-rabbit",
  "version": "4.1.3",
  "description": "White Rabbit - A visual tool for creating Progressive Web Applications",
  "private": true,
  "keywords": [
    "builder",
    "maker",
    "nextjs",
    "react",
    "white-rabbit"
  ],
  "author": "White Rabbit Team",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build": "next build",
    "build:static": "next build && next export",
    "dev": "next dev -p 3012",
    "dev:3020": "next dev -p 3020",
    "check-deps": "node scripts/check-deps.js",
    "restore-deps": "node scripts/restore-deps.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install",
    "commit": "git-cz",
    "start": "next start",
    "deploy": "node scripts/deploy.js",
    "add-license-headers": "node scripts/add-license-headers.js",
    "analyze": "ANALYZE=true npm run build",
    "analyze:dev": "ANALYZE=true npm run dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3012 && electron .\"",
    "electron:build": "npm run build:static && electron-builder",
    "electron:build:mac": "npm run build:static && electron-builder --mac",
    "electron:build:win": "npm run build:static && electron-builder --win",
    "electron:build:linux": "npm run build:static && electron-builder --linux"
  },
